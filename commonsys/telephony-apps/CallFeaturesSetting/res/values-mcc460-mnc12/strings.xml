<resources>
    <!-- Activate unconditional call forward -->
    <string name="CFU_ACT">*72</string>
    <!-- Deactivate unconditional call forward -->
    <string name="CFU_DEACT">*720</string>
    <!-- Activate call forward when busy -->
    <string name="CFB_ACT">*90</string>
    <!-- Deactivate call forward when busy -->
    <string name="CFB_DEACT">*900</string>
    <!-- Activate call forward when no reply -->
    <string name="CFNRY_ACT">*92</string>
    <!-- Deactivate call forward when no reply -->
    <string name="CFNRY_DEACT">*920</string>
    <!-- Activate call forward when not reachable -->
    <string name="CFNRC_ACT">*68</string>
    <!-- Deactivate call forward when not reachable -->
    <string name="CFNRC_DEACT">*680</string>
    <!-- Deactivate all call forward -->
    <string name="CFA_DEACT">*730</string>
    <!-- Activate call waiting -->
    <string name="CW_ACT">*74</string>
    <!-- Deactivate call waiting -->
    <string name="CW_DEACT">*740</string>
</resources>
