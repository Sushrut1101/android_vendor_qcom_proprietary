<resources>

  <!-- Visual states controlling user guidance appearance -->
  <attr name="state_capturing" format="boolean" />
  <attr name="state_direction_negative" format="boolean" />
  <!-- Direction is left or down -->
  <attr name="state_direction_positive" format="boolean" />
  <!-- Direction is right or up -->
  <attr name="state_speed_normal" format="boolean" />
  <attr name="state_speed_medium" format="boolean" />
  <attr name="state_speed_fast" format="boolean" />

  <declare-styleable name="FocusIndicatorStates">
    <attr name="state_focus_complete" format="boolean"/>
    <attr name="state_focus_succeeded" format="boolean"/>
  </declare-styleable>

</resources>
