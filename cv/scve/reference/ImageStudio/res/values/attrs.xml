<resources>
    <!-- Attributes that can be applied to a HorizontalStripGalleryFragment -->
    <declare-styleable name="HorizontalStripGalleryFragmentAttrs">
        <!-- GridView attributes -->
        <attr name="android:rowCount" />
        <attr name="selectorColor" format="color|reference" />
        <!-- Photo gallery attributes -->
        <attr name="galleryName" format="string" />
        <attr name="imageAspectRatio" format="float|fraction|reference" />
        <attr name="cameraIcon" format="reference" />
        <!-- General attributes -->
        <attr name="android:background" />
    </declare-styleable>
    <!-- Special attribute to set on gridview to indicate that non-checked items should be dimmed -->
    <attr name="state_dimmed" />
    <!-- Action button for toolbar -->
    <attr name="actionButtonIcon" format="reference" />
    <!-- Attribute set for the TranslateScaleImageView -->
    <declare-styleable name="TranslateScaleImageView">
        <attr name="minHitRectSize" format="reference|dimension" />
        <attr name="hitRectPadding" format="reference|dimension" />
    </declare-styleable>
    <!-- Attribute set for the MaskView -->
    <declare-styleable name="RemovalMaskView">
        <attr name="type" format="float" type="integer" />
    </declare-styleable>
</resources>
