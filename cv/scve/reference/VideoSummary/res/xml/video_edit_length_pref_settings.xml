<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <qces.qualcomm.qti.com.videosummary.settings.LengthDialogPreference
        android:id="@+id/length_dialog_preference"
        android:dialogLayout="@layout/settings_length_dialog_layout"
        android:key="@string/video_edit_pref_length"
        android:layout="@layout/custom_pref_row_layout"
        android:title="@string/pref_edit_default_length" />
    />

    <ListPreference
        android:defaultValue="@string/video_edit_pref_theme_default_value"
        android:entries="@array/video_edit_pref_theme"
        android:entryValues="@array/video_edit_pref_theme_values"
        android:key="@string/video_edit_pref_theme"
        android:layout="@layout/custom_pref_row_layout"
        android:summary="%s"
        android:title="@string/video_edit_pref_theme" />

</PreferenceScreen>
