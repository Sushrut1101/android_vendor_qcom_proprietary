<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="5G_UWB_Ind_Config[Slot1] : "/>

        <Spinner
                android:id="@+id/uwbSpinnerSlot1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"/>
        <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="5G_Basic_Ind_Config[Slot1] : "/>

        <Spinner
                android:id="@+id/basicSpinnerSlot1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"/>

        <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="5G_UWB_Ind_Config[Slot2] : "/>

        <Spinner
                android:id="@+id/uwbSpinnerSlot2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"/>
        <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="5G_Basic_Ind_Config[Slot2] : "/>

        <Spinner
                android:id="@+id/basicSpinnerSlot2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"/>

        <Switch
            android:id="@+id/enable5gOnSlot1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:checked="false"
            android:enabled="false"
            android:showText="true"
            android:text="Enable 5G on slot1"/>

        <Switch
                android:id="@+id/enable5gOnSlot2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:checked="false"
                android:enabled="false"
                android:showText="true"
                android:text="Enable 5G on slot2"/>
        <Switch
                android:id="@+id/enableEndcOnSlot1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:checked="false"
                android:enabled="false"
                android:showText="true"
                android:text="Enable Endc on slot1"/>

        <Switch
                android:id="@+id/enableEndcOnSlot2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:checked="false"
                android:enabled="false"
                android:showText="true"
                android:text="Enable Endc on slot2"/>

        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
            <TextView
                    android:id="@+id/textEndcStatusSlot1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="12dp"
                    android:text="Endc Status[Slot1] : "/>

            <TextView
                    android:id="@+id/valueEndcStatusSlot1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="12dp"
                    android:text="-"/>
        </LinearLayout>

        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
            <TextView
                    android:id="@+id/textEndcStatusSlot2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="12dp"
                    android:text="Endc Status[Slot2] : "/>

            <TextView
                    android:id="@+id/valueEndcStatusSlot2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="12dp"
                    android:text="-"/>
        </LinearLayout>

        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
