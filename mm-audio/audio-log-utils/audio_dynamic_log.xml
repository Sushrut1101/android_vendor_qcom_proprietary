<audio_dynamic_log_info>
     <!--
     IMPORTANT -
     1. log_mode can be set to MOD to enable logging for all files under a module.
        If set to FILE, each file value will be parsed to determine logging level.
        log_mode:
        "MOD" - enable logging for all files across module.
                log_enable value for files has no significance in this case.
        "FILE" - enable logging for individual files under a module
     -->
     <module name="hal" log_mode="FILE">
         <!-- hal -->
         <file name="audio_hw.c" log_enable="false"/>
