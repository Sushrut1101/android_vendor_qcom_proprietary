<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >

    <corners
        android:radius="0dp"/>

    <gradient
        android:angle="45"
        android:centerX="35%"
        android:centerColor="#000000"
        android:startColor="#E8E8E8"
        android:endColor="#E8E8E8"
        android:type="linear"/>

    <padding
        android:left="8dp"
        android:top="0dp"
        android:right="8dp"
        android:bottom="0dp"/>

    <stroke
        android:width="3dp"
        android:color="#09070D"/>
</shape>
