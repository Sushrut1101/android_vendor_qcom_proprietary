<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <SwitchPreference
        android:key="enable_wigig_switch"
        android:title="@string/pref_enable_wigig"
        android:summaryOff="@string/pref_description_disabled_wigig"
        android:summaryOn="@string/pref_description_enabled_wigig"/>
    <ListPreference
        android:key="wigig_mode"
        android:title="@string/pref_wigig_mode"
        android:dialogTitle="@string/choose_wigig_mode"
        android:entries="@array/wigig_modes"
        android:entryValues="@array/wigig_modes" />
    <Preference
        android:key="softap_config"
        android:title="@string/wifi_tether_configure_ap_text"
        android:enabled="false" />
    <Preference
        android:key="num_connected"
        android:enabled="false" />
</PreferenceScreen>
