  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional wlfw_shadow_reg_v2_cfg_s shadow_reg_v2<QMI_WLFW_MAX_NUM_SHADOW_REG_V2>;
  //! Host memory address for read index update
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional wlfw_rri_over_ddr_cfg_s rri_over_ddr_cfg;
  //! MSI Config for copy engines
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional wlfw_msi_cfg_s msi_cfg<QMI_WLFW_MAX_NUM_CE>;
