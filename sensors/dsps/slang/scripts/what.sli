sub main( @argv)
{

    my $result = 1 + ( 4 * 5);
    print ( "a: $result == 21 ");
    if ( $result == 21) print( "pass\n"); else print( "fail\n");

    $result = ( 1 + 4) * 5;
    print ( "b: $result == 25 ");
    if ( $result == 25) print( "pass\n"); else print( "fail\n");

    my $report_rate = 5;
    my $report_period = 1 / $report_rate;       // check auto convert to float
    print( "c: $report_period == 0.2 ");
    if ( $report_period == 0.2) print( "pass\n"); else print( "fail\n");
}

