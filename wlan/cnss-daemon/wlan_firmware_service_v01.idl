/*****************************************************************************
  @FILE     wlan_firmware_service_v01.idl

  @BRIEF    WLAN Firmware service API between APSS and Modem

  @DESCRIPTION
  This specification documents Major Version 1 of the Qualcomm technologies, Inc
  Messaging Interface (QMI) for WLAN Firmware Service (QMI_WLFW).

  WLAN Host runs on APSS and WLAN Firmware runs on Modem. This service
  is used for downloading WLAN calibration data, WLAN board data and passing
  WLAN on/off status information from APSS to Modem. WLAN HOST plays the role
  of WLFW QMI client and WLAN Firmware plays the role of WLFW QMI server.
  There are two clients using this service. One is in user-space responsible for
  WLAN calibration data and WLAN board data. The other client is in
  kernel-space responsible for WLAN on/off information.

  @Copyright Copyright (c) 2015-2018 Qualcomm Technologies, Inc.
  All Rights Reserved.
  Confidential and Proprietary - Qualcomm Technologies, Inc.

*****************************************************************************/

include "common_v01.idl";

revision 1;


//! Maximum number of copy engine
const QMI_WLFW_MAX_NUM_CE = 12;
//! Maximum number of copy engine service
const QMI_WLFW_MAX_NUM_SVC = 24;
//! Maximum data size
const QMI_WLFW_MAX_DATA_SIZE = 6144;
//! Maximum number of calibration data files
const QMI_WLFW_MAX_NUM_CAL = 5;
//! Maximum string length
const QMI_WLFW_MAX_STR_LEN = 16;
//! Maximum number of shadow registers
const QMI_WLFW_MAX_NUM_SHADOW_REG = 24;
//! Maximum number of shadow registers version 2
const QMI_WLFW_MAX_NUM_SHADOW_REG_V2 = 36;
//! Maximum number of memory regions
const QMI_WLFW_MAX_NUM_MEMORY_REGIONS = 2;
//! Maximum number of memory configuration
const QMI_WLFW_MAX_NUM_MEM_CFG = 2;
//! Maxinum number of memory segment
const QMI_WLFW_MAX_NUM_MEM_SEG = 32;
//! Maximum timestamp length
const QMI_WLFW_MAX_TIMESTAMP_LEN = 32;
//! WLAN MAC address size
const QMI_WLFW_MAC_ADDR_SIZE = 6;
//! Maximum FW build ID length
const QMI_WLFW_MAX_BUILD_ID_LEN = 128;
//! Maximum Function Name length
const QMI_WLFW_FUNCTION_NAME_LEN = 128;
//! Maximum athdiag data size
const QMI_WLFW_MAX_ATHDIAG_DATA_SIZE = 6144;
//! Maxinum number of GPIO
const QMI_WLFW_MAX_NUM_GPIO = 32;
//! Maxinum wfc call status data size
const QMI_WLFW_MAX_WFC_CALL_STATUS_DATA_SIZE = 256;
//! Maximum hang event data size
const QMI_WLFW_MAX_HANG_EVT_SIZE = 784;

//! WLAN driver mode
enum {
  QMI_WLFW_MISSION,
  /**< Mission mode */
  QMI_WLFW_FTM,
  /**< FTM mode */
  QMI_WLFW_EPPING,
  /**< Epping mode */
  QMI_WLFW_WALTEST,
  /**< WAL test mode */
  QMI_WLFW_OFF,
  /**< OFF mode */
  QMI_WLFW_CCPM,
  /**< CCPM unit test */
  QMI_WLFW_QVIT,
  /**< QVIT mode */
  QMI_WLFW_CALIBRATION
  /**< CALIBRATION mode */
} wlfw_driver_mode_enum;

//! Calibration file ID
enum {
  QMI_WLFW_CAL_TEMP_IDX_0,
  /**< temp 25C file */
  QMI_WLFW_CAL_TEMP_IDX_1,
  /**< temp 45C file */
  QMI_WLFW_CAL_TEMP_IDX_2,
  /**< temp 5C file */
  QMI_WLFW_CAL_TEMP_IDX_3,
  /**< temp 65C file */
  QMI_WLFW_CAL_TEMP_IDX_4
  /**< temp -15C file */
} wlfw_cal_temp_id_enum;

//! Pipe direction
enum {
  QMI_WLFW_PIPEDIR_NONE,
  QMI_WLFW_PIPEDIR_IN,
  /**< Target to Host */
  QMI_WLFW_PIPEDIR_OUT,
  /**< Host to Target */
  QMI_WLFW_PIPEDIR_INOUT
  /**< Bidirectional */
} wlfw_pipedir_enum;

enum {
  QMI_WLFW_MEM_TYPE_MSA,
  /**< MSA memory */
  QMI_WLFW_MEM_TYPE_DDR,
  /**< DDR memory */
  QMI_WLFW_MEM_BDF,
  /**< BDF memory */
  QMI_WLFW_MEM_M3,
  /**< M3 memory */
  QMI_WLFW_MEM_CAL,
  /**< Calibration data memory */
  QMI_WLFW_MEM_DPD,
  /**< DPD memory */
  QMI_WLFW_MEM_QDSS
  /**< QDSS trace memory */
} wlfw_mem_type_enum;

enum {
  QMI_WLFW_QDSS_TRACE_OFF,
  /**< QDSS trace off */
  QMI_WLFW_QDSS_TRACE_ON
  /**< QDSS trace on */
} wlfw_qdss_trace_mode_enum;

enum {
  QMI_WLFW_WAKE_REQUEST,
  /**< Wake request */
  QMI_WLFW_WAKE_RELEASE
  /**< Wake release*/
} wlfw_soc_wake_enum;

//! Copy engine attribute flag values
mask32 {
   QMI_WLFW_CE_ATTR_FLAGS                  = 0x00,
   QMI_WLFW_CE_ATTR_NO_SNOOP               = 0x01,
   /**< NonSnooping */
   QMI_WLFW_CE_ATTR_BYTE_SWAP_DATA         = 0x02,
   /**< Byte swap data */
   QMI_WLFW_CE_ATTR_SWIZZLE_DESCRIPTORS    = 0x04,
   /**< Swizzle descriptors */
   QMI_WLFW_CE_ATTR_DISABLE_INTR           = 0x08,
   /**< No interrupt on copy completion */
   QMI_WLFW_CE_ATTR_ENABLE_POLL            = 0x10
   /**< Poll for residue */
} wlfw_ce_attr_mask;

//!WLAN FW mask
mask {
  QMI_WLFW_ALREADY_REGISTERED             = 0x01,
  /**< Already registered */
  QMI_WLFW_FW_READY                       = 0x02,
  /**< FW is ready */
  QMI_WLFW_MSA_READY                      = 0x04,
  /**< MSA configuration is ready */
  QMI_WLFW_FW_MEM_READY                   = 0x08,
  /**< Memory is ready */
  QMI_WLFW_FW_INIT_DONE                   = 0x10
  /**< FW initialization is done */
} wlfw_fw_mask;

//!WLAN dynamic feature mask
mask {
  QMI_WLFW_FW_REJUVENATE                  = 0x01
  /**< FW rejuvenate is enabled */
} wlfw_dynamic_feature_mask;

//! structure for copy engine configuration
struct {
  uint32 pipe_num;
  wlfw_pipedir_enum pipe_dir;
  uint32 nentries;
  uint32 nbytes_max;
  wlfw_ce_attr_mask flags;
} wlfw_ce_tgt_pipe_cfg_s;

//! structure for copy engine service configuration
struct {
  uint32 service_id;
  wlfw_pipedir_enum pipe_dir;
  uint32 pipe_num;
} wlfw_ce_svc_pipe_cfg_s;

//! structure for shadow register
struct {
  uint16 id;
  uint16 offset;
} wlfw_shadow_reg_cfg_s;

//! structure for shadow register version 2
struct {
  uint32 addr;
} wlfw_shadow_reg_v2_cfg_s;

//! structure for read index update memory information
struct {
  uint32 base_addr_low;
  uint32 base_addr_high;
} wlfw_rri_over_ddr_cfg_s;

//! structure for CE MSI configuration
struct {
  uint16 ce_id;
  uint16 msi_vector;
} wlfw_msi_cfg_s;

//! structure for memory region information
struct {
  uint64 region_addr;
  uint32 size;
  uint8 secure_flag;
} wlfw_memory_region_info_s;

//! structure for memory configuration information
struct {
  uint64 offset;
  uint32 size;
  uint8 secure_flag;
} wlfw_mem_cfg_s;

//! structure for memory segment request information
struct {
  uint32 size;
  wlfw_mem_type_enum type;
  wlfw_mem_cfg_s mem_cfg<QMI_WLFW_MAX_NUM_MEM_CFG>;
} wlfw_mem_seg_s;

//! structure for memory segment allocation information
struct {
  uint64 addr;
  uint32 size;
  wlfw_mem_type_enum type;
  boolean restore;
} wlfw_mem_seg_resp_s;

//! structure for RF chip information
struct {
  uint32 chip_id;
  uint32 chip_family;
} wlfw_rf_chip_info_s;

//! structure for RF board information
struct {
  uint32 board_id;
} wlfw_rf_board_info_s;

//! structure for SOC information
struct {
  uint32 soc_id;
} wlfw_soc_info_s;

//! structure for FW version information
struct {
  uint32 fw_version;
  string fw_build_timestamp<QMI_WLFW_MAX_TIMESTAMP_LEN>;
} wlfw_fw_version_info_s;

//============================================================================
/** @COMMAND QMI_WLFW_IND_REGISTER
    @BRIEF This command sends client interested indications
    @CMD_VERSION 1.0
    @USAGE Production
    @SECURITY_LEVEL Low Risk
    @EQUIRED_ACTION Default
*/
//============================================================================

//! @MSG     QMI_WLFW_IND_REGISTER_REQ
//! @TYPE    Request
//! @SENDER  Control point
//----------------------------------------------------------------------------

message {
  //! Subscribe option of wlfw_fw_ready_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean fw_ready_enable;
  //! Subscribe option of wlfw_initiate_cal_download_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean initiate_cal_download_enable;
  //! Subscribe option of wlfw_initiate_cal_update_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean initiate_cal_update_enable;
  //! Subscribe option of wlfw_msa_ready_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean msa_ready_enable;
  //! Subscribe option of wlfw_pin_connect_result_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean pin_connect_result_enable;
  //! Unique client ID
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional uint32 client_id;
  //! Subscribe option of wlfw_request_mem_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean request_mem_enable;
  //! Subscribe option of wlfw_fw_mem_ready_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean fw_mem_ready_enable;
  //! Subscribe option of wlfw_fw_init_done_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean fw_init_done_enable;
  //! Subscribe option of wlfw_rejuvenate_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional uint32 rejuvenate_enable;
  //! Subscribe option of wlfw_xo_cal_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean xo_cal_enable;
  //! Subscribe option of wlfw_cal_done_ing_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean cal_done_enable;
  //! Subscribe option of wlfw_qdss_trace_req_mem_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean qdss_trace_req_mem_enable;
  //! Subscribe option of wlfw_qdss_trace_save_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean qdss_trace_save_enable;
  //! Subscribe option of wlfw_qdss_trace_free_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean qdss_trace_free_enable;
  //! Subscribe option of wlfw_respond_get_info_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean respond_get_info_enable;
  //! Subscribe option of wlfw_m3_dump_upload_req_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean m3_dump_upload_req_enable;
  //! Subscribe option of wlfw_hang_event_ind_msg
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional boolean hang_event_ind_msg;
} wlfw_ind_register_req_msg;

//! @MSG     QMI_WLFW_IND_REGISTER_RESP
//! @TYPE    Response
//! @SENDER  Service
//----------------------------------------------------------------------------

message {
  //! Result Code
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  mandatory qmi_response_type resp;           //!< Standard response type.
  //! FW Status Mask
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional wlfw_fw_mask fw_status;
} wlfw_ind_register_resp_msg;

/** @ERROR
   QMI_ERR_NONE        No error in the request
   QMI_ERR_INTERNAL    Unexpected error occurred during processing
*/

