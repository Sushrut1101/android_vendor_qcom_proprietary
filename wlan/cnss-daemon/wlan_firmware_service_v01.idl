  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional wlfw_ce_tgt_pipe_cfg_s tgt_cfg<QMI_WLFW_MAX_NUM_CE>;
  //! CE Service Configuration Information
  //! @VERSION 1.0
  //! @VERSION_INTRODUCED 1.0
  optional wlfw_ce_svc_pipe_cfg_s svc_cfg<QMI_WLFW_MAX_NUM_SVC>;
