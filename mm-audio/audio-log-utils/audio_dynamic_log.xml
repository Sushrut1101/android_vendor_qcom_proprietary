         <file name="adsp_hdlr.c" log_enable="false"/>
         <file name="audio_extn.c" log_enable="false"/>
         <file name="bt_hal.c" log_enable="false"/>
         <file name="compress_capture.c" log_enable="false"/>
         <file name="compress_in.c" log_enable="false"/>
         <file name="dev_arbi.c" log_enable="false"/>
         <file name="dolby.c" log_enable="false"/>
         <file name="dts_eagle.c" log_enable="false"/>
         <file name="fm.c" log_enable="false"/>
         <file name="gef.c" log_enable="false"/>
         <file name="hfp.c" log_enable="false"/>
