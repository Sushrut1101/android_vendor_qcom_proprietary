<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >

    <ListPreference
        android:defaultValue="@string/setting_default_text_record_mode"
        android:entries="@array/setting_text_record_options"
        android:entryValues="@array/setting_text_record_options"
        android:key="STATextRecordMode"
        android:title="@string/setting_text_record_mode" />

    <MultiSelectListPreference
        android:defaultValue="@array/setting_sta_data_type_values"
        android:entries="@array/setting_sta_data_type_options"
        android:entryValues="@array/setting_sta_data_type_values"
        android:key="STADataTypes"
        android:title="@string/setting_sta_data_type" />

    <ListPreference
        android:defaultValue="@string/setting_default_sta_locale"
        android:entries="@array/setting_sta_locale_type"
        android:entryValues="@array/setting_sta_locale_type_values"
        android:key="STALocaleType"
        android:title="@string/setting_sta_locale_type" />

<!--
    <ListPreference
        android:defaultValue="@string/setting_default_ic_region_ex_mode"
        android:entries="@array/setting_ic_region_ex_modes"
        android:entryValues="@array/setting_ic_region_ex_mode_values"
        android:key="ICRegionExMode"
        android:title="@string/setting_ic_region_ex_mode" />
     -->

    <ListPreference
        android:defaultValue="@string/setting_default_ic_image_en_mode"
        android:entries="@array/setting_ic_image_en_modes"
        android:entryValues="@array/setting_ic_image_en_mode_values"
        android:key="ICImageEnMode"
        android:title="@string/setting_ic_image_en_mode" />

    <EditTextPreference
        android:name="camera_steady_duration"
        android:defaultValue="@integer/default_camera_steady_duration"
        android:digits="0123456789"
        android:inputType="textVisiblePassword"
        android:key="cameraSteadyDuration"
        android:title="@string/setting_camera_steady_duration" />
    <EditTextPreference
        android:name="camera_move_threshold"
        android:defaultValue="@dimen/default_camera_move_threshold"
        android:digits="0123456789"
        android:inputType="textVisiblePassword"
        android:key="cameraMoveThreshold"
        android:title="@string/setting_camera_move_threshold" />

</PreferenceScreen>
